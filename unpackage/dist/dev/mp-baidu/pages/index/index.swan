<view class="content"><swiper indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block s-for="{{carouselList}}" s-for-item="carousel" s-for-index="__i0__"><swiper-item><image class="carousel" src="{{carousel.image}}"></image></swiper-item></block></swiper><view class="page-block super-hot"><view class="hot-title-wapper"><image class="hot-ico" src="../../static/icos/hot.png" mode></image><view class="hot-title">热门超英</view></view></view><scroll-view class="page-block hot" scroll-x="true"><block s-for="{{hotSuperheroList}}" s-for-item="superhero" s-for-index="index" s-key="index"><view class="single-poster"><view class="poster-wapper"><image class="poster" src="{{superhero.cover}}" mode></image><view class="movie-name">{{''+superhero.name+''}}</view><trailer-stars vue-id="{{'1-'+index}}" innerScore="{{superhero.score}}" showNum="1"></trailer-stars></view></view></block></scroll-view><view class="page-block super-hot"><view class="hot-title-wapper"><image class="hot-ico" src="../../static/icos/interest.png" mode></image><view class="hot-title">热门预告</view></view></view><view class="hot-movies page-block"><block s-for="{{hotTrailerList}}" s-for-item="trailer" s-for-index="__i1__"><video class="hot-movie-single" src="{{trailer.trailer}}" poster="{{trailer.poster}}" controls="controls"></video></block></view><view class="page-block super-hot"><view class="hot-title-wapper"><image class="hot-ico" src="../../static/icos/guess-u-like.png" mode></image><view class="hot-title">猜你喜欢</view></view></view><view class="page-block guess-u-like"><block s-for="{{guessULikeList}}" s-for-item="guess" s-for-index="gIndex"><view class="single-like-movie"><image class="like-movie" src="{{guess.cover}}" mode></image><view class="movie-desc"><view class="movie-title">{{''+guess.name+''}}</view><trailer-stars vue-id="{{'2-'+gIndex}}" innerScore="{{guess.score}}" showNum="0"></trailer-stars><view class="movie-info">{{''+guess.basicInfo+''}}</view><view class="movie-info">{{''+guess.releaseDate+''}}</view></view><view class="movie-oper" data-gIndex="{{gIndex}}" data-event-opts="{{[['tap',[['praiseMe',['$event']]]]]}}" bindtap="__e"><image class="praise-ico" src="../../static/icos/praise.png"></image><view class="praise-me">赞一下</view><view class="praise-me animation-opacity" animation="{{animationDataArr[gIndex]}}"></view></view></view></block></view></view>