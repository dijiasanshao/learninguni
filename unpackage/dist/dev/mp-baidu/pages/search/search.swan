<view class="page"><view class="search-block"><view class="search-ico-wapper"><image class="search-ico" src="../../static/icos/search.png" mode></image></view><input class="search-text" type="text" value="" placeholder="搜索预告" maxlength="12" confirm-type="search" data-event-opts="{{[['confirm',[['searchMe',['$event']]]]]}}" bindconfirm="__e"/></view><view class="movie-list page-block"><block s-for="{{trailerList}}" s-for-item="trailer" s-for-index="__i0__"><view class="movie-wapper"><navigator to></navigator><image class="poster" src="{{trailer.cover}}" mode data-trailerId="{{trailer.id}}" data-event-opts="{{[['tap',[['showTrailer',['$event']]]]]}}" bindtap="__e"></image><view class="movie-info">{{''+trailer.name+''}}</view></view></block></view></view>